  },
  "devDependencies": {
    "@nestjs/testing": "^10.2.1",
    "@types/jest": "^27.0.0",
    "@types/mock-fs": "^4.10.0",
    "jest": "^29.0.0",
    "mock-fs": "^5.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0"
  }
}
