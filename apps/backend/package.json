{
  "name": "heimdall-server",
  "version": "2.10.19",
  "description": "",
  "license": "Apache-2.0",
  "author": "",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "eslint \"{src,migrations,seeders,test}/**/*.ts\" --fix",
    "lint:ci": "eslint \"{src,migrations,seeders,test}/**/*.ts\" --max-warnings 0",
    "sequelize-cli-windows": "tsx node_modules/sequelize-cli/lib/sequelize",
    "sequelize-cli": "tsx node_modules/.bin/sequelize",
    "start": "node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:fips": "node --force-fips dist/src/main",
    "test": "jest --silent",
    "test:ci-cov": "jest --silent --coverage --runInBand",
    "test:cov": "jest --silent --coverage",
    "test:debug": "tsx --inspect-brk node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --silent --config test/jest-e2e.json",
    "test:log": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "testTimeout": 10000,
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@casl/ability": "^6.0.0",
    "@heimdall/common": "^2.10.16",
    "@heimdall/password-complexity": "^2.6.6",
    "@nestjs/cli": "^10.1.14",
    "@nestjs/common": "^10.2.1",
    "@nestjs/core": "^10.2.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.17.3",
    "@types/js-levenshtein": "^1.1.0",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.5",
    "@types/node": "^22.0.0",
    "@types/passport-github": "^1.1.5",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^6.0.1",
    "@types/triple-beam": "^1.3.2",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.0.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^16.0.0",
    "eslint-plugin-import": "^2.20.1",
    "express-rate-limit": "^7.0.0",
    "express-session": "^1.17.1",
    "helmet": "^7.1.0",
    "js-levenshtein": "^1.1.6",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.1",
    "ms": "^2.1.3",
    "passport": "^0.7.0",
    "passport-github": "^1.1.0",
    "passport-gitlab2": "^5.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "passport-ldapauth": "^3.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "passport-openidconnect": "^0.1.1",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.0",
    "rxjs": "^7.2.0",
    "sequelize": "^6.31.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.0.0",
    "ts-loader": "^9.1.0",
    "tsx": "^4.7.1",
